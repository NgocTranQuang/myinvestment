<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="badgedBottomNavigationBar">
        <attr name="badge_layout" format="reference" />
    </declare-styleable>


    <declare-styleable name="custom_rv_with_indicator">
        <attr name="color_selected" format="color" />
        <attr name="color_unSelected" format="color" />
        <attr name="isAuto_slide" format="boolean" />
        <attr name="square_cardview" format="boolean" />
        <attr name="time_silde" format="float" />
        <attr name="scale" format="float" />
        <attr name="stype" format="integer" />
        <attr name="show_image_favourite" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PinViewTheme">
        <attr name="pinViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="PinView">
        <attr name="itemCount" format="integer" />
        <attr name="itemWidth" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="itemRadius" format="dimension" />
        <attr name="itemSpacing" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="reference|color" />
        <attr name="viewType" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="line" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="android:cursorVisible" />
        <attr name="cursorWidth" format="dimension" />
        <attr name="cursorColor" format="reference|color" />
        <attr name="android:itemBackground" />
        <attr name="hideLineWhenFilled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AvatarImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ViewTitle">
        <attr name="title" format="string" />
        <attr name="hint" format="string" />
        <attr name="message_error" format="string" />
        <attr name="disable" format="boolean" />
        <attr name="checked" format="boolean" />
        <attr name="singleLine" format="boolean" />
        <attr name="collapsing" format="boolean" />
        <attr name="visibleLine" format="boolean" />
        <attr name="min_height" format="dimension" />
        <attr name="inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="date" value="3" />
            <enum name="time" value="4" />
            <enum name="datetime" value="5" />
            <enum name="HTML" value="6" />
        </attr>

        <attr name="chooseType" format="enum">
            <enum name="singleChoice" value="0" />
            <enum name="multiChoice" value="1" />
        </attr>
        <attr name="skip_showing" format="boolean" />
    </declare-styleable>

    <declare-styleable name="QuantityEditView">
        <attr name="max_value" format="integer" />

    </declare-styleable>

    <declare-styleable name="ProfileRow">
        <attr name="title_profile" format="string" />
        <attr name="src_profile" format="reference" />
        <!--        <attr name="title2" format="string" />-->
        <!--        <attr name="count" format="integer" />-->
        <!--        <attr name="inputType" format="enum">-->
        <!--            <enum name="text" value="0" />-->
        <!--            <enum name="number" value="1" />-->
        <!--            <enum name="date" value="3" />-->
        <!--            <enum name="endDate" value="4" />-->
        <!--            <enum name="datetime" value="5" />-->
        <!--        </attr>-->
    </declare-styleable>

    <declare-styleable name="ReflowText">
        <attr name="velocity" format="dimension" />
        <attr name="minDuration" format="integer" />
        <attr name="maxDuration" format="integer" />
        <attr name="staggerDelay" format="integer" />
        <attr name="freezeFrame" format="boolean" />
    </declare-styleable>

    <attr name="max" format="integer" />
    <attr name="shadow" format="boolean" />
    <attr name="progressThumb" format="boolean" />
    <attr name="startingAngle" format="integer" />
    <attr name="progress" format="integer" />
    <attr name="progressBarThickness" format="dimension" />
    <attr name="progressBarColor" format="color" />
    <attr name="progressBarColorArray" format="reference" />
    <attr name="progressBarColorArrayPositions" format="reference" />
    <attr name="duplicateFirstColorInArray" format="boolean" />
    <attr name="progressBarRounded" format="boolean" />
    <attr name="backgroundColor" format="color" />
    <attr name="backgroundAlphaEnabled" format="boolean" />
    <attr name="reverse" format="boolean" />

    <declare-styleable name="CircularProgressView">
        <attr name="max" />
        <attr name="shadow" />
        <attr name="progressThumb" />
        <attr name="startingAngle" />
        <attr name="progress" />
        <attr name="progressBarThickness" />
        <attr name="progressBarColor" />
        <attr name="progressBarColorArray" />
        <attr name="progressBarColorArrayPositions" />
        <attr name="duplicateFirstColorInArray" />
        <attr name="progressBarRounded" />
        <attr name="backgroundColor" />
        <attr name="backgroundAlphaEnabled" />
        <attr name="reverse" />
    </declare-styleable>

    <declare-styleable name="ListSwipeItem">
        <attr name="swipeViewId" format="integer" />
        <attr name="leftViewId" format="integer" />
        <attr name="rightViewId" format="integer" />
    </declare-styleable>

    <!--<editor-fold desc="Collapsible calendar">-->
    <declare-styleable name="ExpandIconView">
        <attr name="eiv_roundedCorners" format="boolean" />
        <attr name="eiv_switchColor" format="boolean" />
        <attr name="eiv_color" format="color" />
        <attr name="eiv_colorMore" format="color" />
        <attr name="eiv_colorLess" format="color" />
        <attr name="eiv_colorIntermediate" format="color" />
        <attr name="eiv_animationDuration" format="integer" />
        <attr name="eiv_padding" format="dimension" />
    </declare-styleable>

    <!--</editor-fold>-->

</resources>